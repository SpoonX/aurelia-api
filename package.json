    "url": "https://github.com/SpoonX/aurelia-api"
  },
  "devDependencies": {
    "aurelia-tools": "^0.1.18",
    "babel-eslint": "^4.1.3",
    "body-parser": "^1.14.2",
    "conventional-changelog": "0.0.17",
    "cors": "^2.7.1",
    "del": "^2.2.0",
    "express": "^4.13.3",
    "gulp": "^3.8.10",
    "gulp-babel": "^5.1.0",
    "gulp-bump": "^0.3.1",
    "gulp-eslint": "^1.1.1",

    "gulp-yuidoc": "^0.1.2",
    "jasmine-core": "^2.1.3",
    "karma": "^0.13.15",
